            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListaDepartamentosActivity">

        </activity>
        <receiver android:name=".ReceptorAlarma">
            <intent-filter>
                <action android:name="com.dam.kevoundfreunde.laboratorio04.ListaDepartamentosActivity.ReceptorAlarma" />
            </intent-filter>
        </receiver>
        <activity android:name=".AltaReservaActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
    </application>

</manifest>